{
    "name": "frontend",
    "private": true,
    "version": "1.0.2",
    "type": "module",
    "scripts": {
        "dev": "node --env-file-if-exists=./../../.env ../../node_modules/vite/bin/vite.js",
        "serve": "serve dist -p 3000",
        "test": "vitest --config vitest.config.ts run",
        "test-coverage-html": "vitest run --coverage --reporter=html",
        "install-playwright-ci": "npx playwright install --with-deps",
        "test-playwright": "npx playwright test",
        "make-badges": "istanbul-badges-readme",
        "tsc": "tsc --noEmit",
        "build": "tsc -b && node --env-file-if-exists=./../../.env ../../node_modules/vite/bin/vite.js build",
        "lint": "eslint --config eslint.config.mjs",
        "prettier": "npx prettier src/**/*.{ts,tsx}",
        "preview": "node --env-file-if-exists=./../../.env ../../node_modules/vite/bin/vite.js preview"
    },
    "dependencies": {
        "@mantine/charts": "^8.0.2",
        "@mantine/code-highlight": "^8.0.2",
        "@mantine/core": "^8.0.2",
        "@mantine/dates": "^8.0.2",
        "@mantine/dropzone": "^8.0.2",
        "@mantine/form": "^8.0.2",
        "@mantine/hooks": "^8.0.2",
        "@mantine/modals": "^8.0.2",
        "@mantine/notifications": "^8.0.2",
        "@mantine/nprogress": "^8.0.2",
        "@mantine/spotlight": "^8.0.2",
        "@mantine/tiptap": "^8.0.2",
        "@tiptap/extension-link": "^2.14.0",
        "@tiptap/pm": "^2.14.0",
        "@tiptap/react": "^2.14.0",
        "@tiptap/starter-kit": "^2.14.0",
        "dayjs": "^1.11.13",
        "i18next": "^25.2.1",
        "monaco-editor": "^0.52.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-i18next": "^15.5.2",
        "react-router": "^7.6.2",
        "recharts": "^2.15.3",
        "serve": "^14.2.4",
        "socket.io-client": "^4.8.1",
        "yargs": "^18.0.0",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@playwright/test": "^1.52.0",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^22.15.30",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.6.0",
        "electron": "^36.4.0",
        "eslint": "^9.28.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "global-jsdom": "^26.0.0",
        "globals": "^16.2.0",
        "istanbul-badges-readme": "^1.9.0",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.4",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.33.1",
        "vite": "^6.3.5",
        "vitest": "^3.2.2"
    },
    "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
    "main": "eslint.config.js",
    "author": "Giuseppe Giulio Caponetto",
    "license": "MIT"
}
